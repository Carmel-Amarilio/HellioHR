{
  "name": "hellio-hr-backend",
  "version": "1.0.0",
  "description": "Node.js + Express backend for Hellio HR",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:integration": "vitest run --config vitest.config.ts",
    "test:watch": "vitest watch",
    "test:bedrock": "tsx scripts/test-bedrock.ts",
    "ingest": "tsx scripts/ingest-all.ts",
    "ingest:cvs": "tsx scripts/ingest-all.ts --cvs",
    "ingest:jobs": "tsx scripts/ingest-all.ts --jobs",
    "ingest:heuristic": "tsx scripts/ingest-all.ts --heuristic-only",
    "demo": "tsx scripts/demo-extraction.ts",
    "demo:real": "tsx scripts/demo-extraction.ts --real",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.975.0",
    "@prisma/client": "^6.8.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "multer": "^2.0.2",
    "node-sql-parser": "^5.4.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.13.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "prisma": "^6.8.0",
    "supertest": "^7.1.0",
    "tsx": "^4.19.4",
    "typescript": "~5.9.3",
    "vitest": "^4.0.17"
  }
}
